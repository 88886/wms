.wrapper{
  font-size:12px;
}
.wrapper .msg{padding:20px 20px 20px;background-color:#ecefed}
.wrapper .msg .head{box-shadow:0px 10px 60px #DFDFDF;border-radius:3px;background-color:#fdfdfd;}
.wrapper .msg .tip{text-align:center;font-size:20px;height:120px;padding-top:44px}
.wrapper .msg .code{height:20px;padding:5px;color:#555555}
.wrapper .msg .nav{height:40px;line-height:40px;margin-top:20px}
.wrapper .msg .btn{text-align:center;border-radius:3px;width:130px;background-color:#0986f0;color:#FFFFFF;}
.wrapper .info{padding:10px 20px;margin-top:10px;background-color:#ecefed;color:#999}